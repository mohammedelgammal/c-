#ifndef ALPHABETA
#define ALPHABETA

class Alpha;

class Beta
{
public:
    Beta() : bvalue{15} {}
    friend class Alpha;

private:
    int bvalue;
};
class Alpha
{
public:
    Alpha() : avalue{10} {};
    int getBetaVal(Beta b)
    {
        return b.bvalue;
    };

private:
    int avalue;
};

#endif // ALPHABETA