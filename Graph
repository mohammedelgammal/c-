#ifndef GRAPH
#define GRAPH

#include <iostream>
#include <list>
using namespace std;

struct Node
{
    Node(const string label) : label{label} {};
    const string label;
};

class Graph
{
public:
    Graph();
    ~Graph();

private:
    unordered_map<string, list<Node *>> *graph;
};

#endif // GRAPH