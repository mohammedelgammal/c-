#ifndef QUEUESTACK
#define QUEUESTACK

#include "Stack"
#include "Stack.cpp"

template <typename T>
class QueueStack
{
public:
    // Constructors
    QueueStack();
    QueueStack(const T value);
    ~QueueStack();

    // Methods
    void enqueue(T value);
    void dequeue();
    T getPeak() const;
    T getBottom() const;
    bool isEmpty() const;
    int getLength() const;
    T operator[](const int index) const;

private:
    Stack<T> *stack;
};

#endif // QUEUESTACK