#ifndef ITERATOR
#define ITERATOR

#include <iostream>
#include <vector>
using namespace std;

class BrowserHistory;

class ListIterator
{
public:
    ListIterator(BrowserHistory *history);
    void next();
    bool hasNext();
    string getCurrent();

private:
    int index = 0;
    BrowserHistory *history;
};

class BrowserHistory
{
public:
    void add(const string content);
    string pop();
    ListIterator *getIterator(BrowserHistory *history) const;
    friend class ListIterator;

private:
    vector<string> _list;
};

#endif // ITERATOR