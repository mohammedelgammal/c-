#ifndef PRIORITYQUEUEHEAP
#define PRIORITYQUEUEHEAP

#include "Heap"

class PriorityQueueHeap
{
public:
    // Constructors
    PriorityQueueHeap(int root);
    ~PriorityQueueHeap();
    // Methods
    void enqueue(const int value);
    int dequeue();
    bool isEmpty() const;

private:
    Heap *heap;
};

#endif // PRIORITYQUEUEHEAP