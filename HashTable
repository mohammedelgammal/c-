#ifndef HASHTABLE
#define HASHTABLE

#include "LinkedList"
#include "LinkedList.cpp"

struct Entry
{
    int key;
    string value;
};

class HashTable
{
public:
    // Constructors
    HashTable(const size_t size);
    ~HashTable();

    // Methods
    int hash(const int key) const;
    void put(const int key, const string value);

private:
    int size;
    LinkedList<Entry> *array[];
};

#endif // HASHTABLE