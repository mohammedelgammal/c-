#ifndef HASHTABLE
#define HASHTABLE

#include "Array"

template <typename K, typename V>
class Node
{
public:
    // Constructors
    Node();
    Node(const K key, const V value);

private:
    K key;
    V value;
};

template <typename K, typename V>
class HashTable
{
public:
    // Constructors
    HashTable();
    ~HashTable();

    // Methods
    int hash(const K key) const;
    void put(const K key, const V value);

private:
    Array<Node<K, V>> *list;
};

#endif // HASHTABLE