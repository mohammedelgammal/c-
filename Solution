#ifndef SOLUTION
#define SOLUTION

using namespace std;

#include <vector>
#include <unordered_map>
#include <stack>

class MyQueue
{
public:
    stack<int> stack, stack2;
    MyQueue()
    {
    }

    void push(int x)
    {
        stack.push(x);
    }

    int pop()
    {
        while (stack.size() > 1)
        {
            stack2.push(stack.top());
            stack.pop();
        }
        int back = stack.top();
        stack.pop();
        while (!stack2.empty())
        {
            stack.push(stack2.top());
            stack2.pop();
        }
        return back;
    }

    int peek()
    {
        while (stack.size() > 1)
        {
            stack2.push(stack.top());
            stack.pop();
        }
        int back = stack.top();
        while (!stack2.empty())
        {
            stack.push(stack2.top());
            stack2.pop();
        }
        return back;
    }

    bool empty()
    {
        return stack.empty();
    }
};

#endif // SOLUTION