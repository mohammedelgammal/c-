#ifndef SOLUTION
#define SOLUTION

using namespace std;

#include <vector>
#include <unordered_map>
#include <queue>

class Solution
{
public:
    int k;
    priority_queue<int, vector<int>, greater<int>> pq;
    Solution(int k, vector<int> &nums)
    {
        this->k = k;
        for (int num : nums)
            pq.push(num);
        while (pq.size() > k)
            pq.pop();
    }
    int add(int val);
};

#endif // SOLUTION