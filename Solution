#ifndef SOLUTION
#define SOLUTION

using namespace std;

#include <vector>
#include <unordered_map>
#include <deque>

class Node
{
public:
    int val;
    Node *next;
    Node *random;

    Node(int _val)
    {
        val = _val;
        next = NULL;
        random = NULL;
    }
};

class Solution
{
public:
    Node *copyRandomList(Node *head);
};

#endif // SOLUTION