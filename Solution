#ifndef SOLUTION
#define SOLUTION

#include <functional>

using std::function;
using std::max;
using std::string;
using std::unordered_map;
using std::vector;
using std::swap;

string maximumOddBinaryNumber(string s) {
  int size = s.size();
  int l = 0, r = 0;
  while (r < size - 1) {
    if (s[r] == '1')
      swap(s[l++], s[r]);
    r++;
  }
  if (l - 1 >= 0)
    swap(s[l - 1], s[size - 1]);
  return s;
}

#endif // SOLUTION